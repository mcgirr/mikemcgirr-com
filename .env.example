# Deployment configuration
# Copy this file to .env and update with your actual values

# SSH server details (username@ip)
DEPLOY_SERVER=username@0.0.0.0

# SSH port
DEPLOY_PORT=22

# Remote directory paths
DEPLOY_TEMP_DIR=/home/username
DEPLOY_FINAL_DIR=/usr/share/nginx/your-site

# SSH key path
SSH_KEY_PATH=/path/to/your/ssh/key
